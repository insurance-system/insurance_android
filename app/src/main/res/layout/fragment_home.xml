<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ui.view.insurance.HomeFragment"
    android:id="@+id/fragment_home">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/home_worker_tv"
                android:text="관리자 페이지 입니다"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:textSize="16sp"
                android:layout_marginTop="20dp"/>

            <LinearLayout
                android:id="@+id/home_new_customer_get_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/home_worker_tv"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="상담 대기 신규 고객 명단 조회"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_new_customer_get_ll"/>

            <LinearLayout
                android:id="@+id/home_edu_upload_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/setting_divider1"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="영업 교육 강의 업로드"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_edu_upload_ll"/>

            <LinearLayout
                android:id="@+id/home_edu_print_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/setting_divider2"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="강의 자료 리스트 출력"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_edu_print_ll"/>


            <LinearLayout
                android:id="@+id/home_ins_state_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/setting_divider3"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="보험 계약 상태"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_ins_state_ll"/>


            <LinearLayout
                android:id="@+id/home_last_customer_get_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/setting_divider4"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="보험 만기 고객 조회"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_last_customer_get_ll"/>

            <LinearLayout
                android:id="@+id/home_not_customer_get_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/setting_divider5"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="미납 고객 조회"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider6"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_not_customer_get_ll"/>

            <LinearLayout
                android:id="@+id/home_check_go_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/setting_divider6"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="인수 심사 수행"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider7"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_check_go_ll"/>


            <LinearLayout
                android:id="@+id/home_accident_check_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/setting_divider7"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="사고 발생 신고 접수확인"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider8"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_accident_check_ll"/>


            <LinearLayout
                android:id="@+id/home_money_check_ll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginHorizontal="20dp"
                app:layout_constraintTop_toBottomOf="@+id/setting_divider8"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="30dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="보상금 심사"
                    android:textSize="20sp"
                    />

            </LinearLayout>

            <View
                android:id="@+id/setting_divider9"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E4E4E7"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/home_money_check_ll"/>
        </androidx.constraintlayout.widget.ConstraintLayout>



    </ScrollView>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_favorite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:src="@drawable/ic_baseline_create_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>