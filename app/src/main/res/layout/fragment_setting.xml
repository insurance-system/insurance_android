<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ui.view.insurance.SettingFragment"
    android:id="@+id/fragment_setting">


    <LinearLayout
        android:id="@+id/setting_logout_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="30dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="로그아웃"
            android:textSize="20sp"
            />

    </LinearLayout>

    <View
        android:id="@+id/setting_divider1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E4E4E7"
        android:layout_marginTop="15dp"
        android:layout_marginHorizontal="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setting_logout_ll"/>

    <LinearLayout
        android:id="@+id/setting_question_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintTop_toBottomOf="@+id/setting_divider1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="30dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="문의하기"
            android:textSize="20sp"
            />

    </LinearLayout>

    <View
        android:id="@+id/setting_divider2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E4E4E7"
        android:layout_marginTop="15dp"
        android:layout_marginHorizontal="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setting_question_ll"/>

</androidx.constraintlayout.widget.ConstraintLayout>